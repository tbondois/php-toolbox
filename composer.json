{
    "name": "tbondois/php-toolbox",
    "description": "PHP libraries for common needs",
    "require": {
        "php": ">=7.0",
        "ext-json": "*",
        "symfony/polyfill-ctype": "^1.11",
        "brandonwamboldt/utilphp": "^1.1"
    },
    "require-dev": {
        "tbondois/php-debug": "*"
    },
    "minimum-stability": "stable",
    "suggest": {
        "tbondois/php-debug": "PHP debug libraries (dumpers...) for common needs. should not be installed in production",
        "ext-json": "Needed to use Json class"
    },
    "autoload": {
        "psr-4": {
            "TB\\Toolbox\\": "src/"
        },
        "files": [
            "init/aliases.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "TB\\Toolbox\\Tests\\": "tests/"
        }
    },
    "license": "MIT",
    "homepage": "https://packagist.org/packages/tbondois/php-toolbox",
    "authors": [
        {
            "name": "Thomas Bondois",
            "homepage": "https://thomas.bondois.info"
        }
    ],
    "keywords": [
        "polyfill",
        "json",
        "util",
        "utils",
        "toolbox",
        "php"
    ]
}
